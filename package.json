    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint .",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garrettjoecox/skeleton-server.git"
  },
  "author": "Garrett Cox <garrett@clevyr.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/garrettjoecox/skeleton-server/issues"
  },
  "homepage": "https://github.com/garrettjoecox/skeleton-server#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "koa": "^2.13.0",
    "lodash": "^4.17.21",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/koa": "^2.11.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
